#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
#include <time.h>
int main(){
int i, arr[20];
printf("20 so ngau nhien la: ");
srand((int)time(0));
for(i = 0; i < 20; i++){
 arr[i] = rand();
printf("%d, ", arr[i]);
}
int a, b;
printf("\n\nNhap gia tri xem xet:\n");
scanf("%d", &a);
for(i = 0; i <20; i++){
if(a == arr[i])
b = 1;
}if(b == 0){
printf("\n%d khong thuoc mang so ngau nhien", a);
}else{
printf("\n%d thuoc mang so ngau nhien", a);
}
return 0;
}
